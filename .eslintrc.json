{
  "root": true,
  "env": {
    "node": true
  },
  "extends": ["plugin:vue/vue3-essential", "eslint:recommended"],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "no-unused-vars": "off",
    "vue/no-unused-components": "warn",
    "no-unused-labels": "off",
    "vue/no-mutating-props": "off"
  }
}
