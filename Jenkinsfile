pipeline {
    agent any
    stages{
         stage('Checkout Stage') {
            steps:{
                echo: "Building Container.."
             }
        }
        stage('Build Stage') {
            steps{
                echo "---Build Stage222---"
            }
            
        }
        stage('Push Stage') {
            steps{
                echo "---Push Stage2222---"
            }       
        }
    }
   
}